{
   "background_page": "html/background.html",
   "options_page": "html/options.html",
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/jquery.min.js", "js/app.js" ],
      "matches": [ "http://mail.google.com/*", "https://mail.google.com/*" ],
      "run_at": "document_idle"
   } ],
   "description": "Group your Chat Buddies and chat seamlessly",
   "icons": {
      "128": "images/logo_128.gif",
      "19": "images/logo_19.gif",
      "64": "images/logo_64.gif",
      "48": "images/logo_48.gif"
   },
   "permissions": [ "tabs", "contextMenus", "management", "http://www.google.com/m8/feeds/*", "https://www.google.com/accounts/OAuthGetRequestToken", "https://www.google.com/accounts/OAuthAuthorizeToken", "https://www.google.com/accounts/OAuthGetAccessToken" ],
   "name": "Chatify",
   "version": "0.1"
}
